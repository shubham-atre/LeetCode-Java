//O(N)
class Solution {
    public int maxProfit(int[] prices) {
        
        if(prices.length < 2)
            return 0;
        
        int min = prices[0];
               
        int max = Integer.MIN_VALUE;
        
        for(int i:prices)
        {
            if(i<min)
                min=i;
            max = Math.max(max, i-min);
        }
        
        return max;
        
    }
}
